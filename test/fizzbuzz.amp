use "std"

fn main() {
    let i = 0

    while i < 10 {
        i = i + 1
        println(fizzbuzz(i))
    }
}

fn fizzbuzz(int: integer): String {
    if int % 15 == 0 {
        return "FizzBuzz"
    } else if int % 5 == 0 {
        return "Buzz"
    } else if int % 3 == 0 {
        return "Fizz"
    }
    return int.to_string()
}
