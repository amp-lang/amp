use "std"

fn main() {
    let i = 0

    while (i < 10) {
        i = i + 1
        let fizzbuzz = fizzbuzz(i)
        println(fizzbuzz)
    }
}

fn fizzbuzz(int: integer): String {
    if ((int % 15) == 0) {
        return "FizzBuzz"
    }
    if ((int % 5) == 0) {
        return "Buzz"
    }
    if ((int % 3) == 0) {
        return "Fizz"
    }
    return integer_to_string(int)
}
