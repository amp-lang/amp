use "std.amp";

fn main() {
    let parameters = Array.new(1);
    parameters.set(0, "Welcome child!");

    let functions = Array.new(2);
    functions.set(0, test);
    functions.set(1, test2);

    let i = 0;
    while (less(i, Array.length(functions))) {
        let function = Function(Array.get(functions, i));
        println(String(function.call(parameters)));
        i = integer(add(i, 1));
    };
};

fn test(test: String) {
    return concatenate("Hello from test function! Main function says: ", test);
};

fn test2(test: String) {
    return concatenate("Hello from test2 function! Main function says: ", test);
};