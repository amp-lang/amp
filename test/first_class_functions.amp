use "std.amp";

fn main() {
    let functions = Array.new(2);
    functions.set(0, test);
    functions.set(1, test2);

    let i = 0;
    while (i < functions.length()) {
    	let parameters = Array.new(1);
    	parameters.set(0, "Welcome child!");

		println(@cast_String(@cast_&Function(functions.get(i)).call(parameters)));
        i = i + 1;
    };
};

fn test(test: String): String {
    return "Hello from test function! Main function says: " + test;
};

fn test2(test: String): String {
    return "Hello from test2 function! Main function says: " + test;
};
