use "std.lang";

fn main() {
    process(4);
    process(3);
    process(2);
    process(1);
};

fn process(value: integer) {
    if (equal(value, 4)) {
        println("is 4!");
    } else if (equal(value, 3)) {
        println("is 3!");
    } else if (equal(value, 2)) {
        println("is 2!");
    } else {
        println("is thing!");
    };
};