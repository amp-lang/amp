use "std.amp";

fn main() {
    let list = LinkedList.new();
    list.append(5);
    list.append(3);
    list.append(2);
    list.append(1);
    list.append(4);
    list.insert(0, 4);
    list.insert(2, 4);
    list.remove(1);
    print_int_list(list);
    println("List size: " + integer_to_string(list.size()));
    println("At 0: " + integer_to_string(@cast_integer(list.get(0))));
};

fn print_int_list(list: LinkedList) {
    let node = list.head();
    while (not_equal(node.next(), null())) {
        node = node.next();
        println(integer_to_string(@cast_integer(node.data())));
    };
};