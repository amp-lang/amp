use "std.amp";

fn main() {
    let option = Option.empty();

    println(boolean_to_string(option.is_value()));

    option = Option.of("Hello Option!", String.memory_size());
    if (option.is_value()) {
        println(@cast_&String(option.data));
    } else {
        println("not a thing (this should never print with this code)");
    };
};
