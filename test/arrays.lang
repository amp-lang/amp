use "std.lang";

fn main() {
    let array = Array.new(2);
    Array.set(array, 0, "Hello ");
    Array.set(array, 1, "World!");
    println(concatenate(String(Array.get(array, 0)), String(Array.get(array, 1))));
    println(concatenate("Array Length: ", integer_to_string(integer(Array.length(array)))));
};